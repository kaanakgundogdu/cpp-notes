.PHONY: clean All

All:
	@echo "----------Building project:[ 25-While-Loops - Debug ]----------"
	@cd "25-While-Loops" && "$(MAKE)" -f  "25-While-Loops.mk"
clean:
	@echo "----------Cleaning project:[ 25-While-Loops - Debug ]----------"
	@cd "25-While-Loops" && "$(MAKE)" -f  "25-While-Loops.mk" clean
