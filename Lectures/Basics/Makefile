.PHONY: clean All

All:
	@echo "----------Building project:[ 06-GlobalVariables - Debug ]----------"
	@cd "06-GlobalVariables" && "$(MAKE)" -f  "06-GlobalVariables.mk"
clean:
	@echo "----------Cleaning project:[ 06-GlobalVariables - Debug ]----------"
	@cd "06-GlobalVariables" && "$(MAKE)" -f  "06-GlobalVariables.mk" clean
